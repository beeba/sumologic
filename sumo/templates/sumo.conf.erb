#
# from puppet, sumo/templates/sumo.conf.erb
# sumo/manifests/config.pp

name=<%= fqdn %>
accessid=ENTER_YOUR_ACCESSID_HERE
accesskey=1234567890_YOURACCESS_KEY_HERE_0987654321

#this is useful for getting all the same info from your hosts
syncSources=/etc/sources.json

# if you don't have this and you rebuild a server it'll have
# the same name plus a timestamp appended to it
clobber=true

